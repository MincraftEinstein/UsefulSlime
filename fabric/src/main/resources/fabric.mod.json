{
  "schemaVersion": 1,
  "id": "usefulslime",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "http://curseforge.com/minecraft/mc-mods/useful-slime-fabric",
    "sources": "https://github.com/MincraftEinstein/UsefulSlime",
    "issues": "https://github.com/MincraftEinstein/UsefulSlime/issues"
  },
  "license": "Custom license",
  "icon": "pack.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "einstein.usefulslime.UsefulSlimeFabric"
    ],
    "client": [
      "einstein.usefulslime.UsefulSlimeFabric"
    ]
  },
  "mixins": [
    "usefulslime.fabric.mixins.json",
    "usefulslime.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_version}",
    "fabric": ">=${fabric_api_version}",
    "minecraft": ">=${minecraft_version} <${max_minecraft_version}",
    "java": ">=${java_version}",
    "fzzy_config": ">=${fzzy_config_version}",
    "commonnetworking": ">=${networking_version}-${minecraft_version}"
  },
  "custom": {
    "catalogue": {
      "banner": "pack.png"
    },
    "modmenu": {
      "links": {
        "modmenu.usefulslime.license": "https://github.com/MincraftEinstein/UsefulSlime/blob/main/LICENSE.md",
        "modmenu.modrinth": "https://modrinth.com/mod/useful-slime",
        "modmenu.curseforge": "https://curseforge.com/minecraft/mc-mods/useful-slime",
        "modmenu.discord": "https://discord.gg/gSsaFAvrBM",
        "modmenu.kofi": "https://ko-fi.com/mincrafteinstein"
      }
    },
    "fzzy_config": [
      "subtle_effects"
    ]
  }
}
